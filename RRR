local VLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/vep1032/VepStuff/main/VL"))()
local w = VLib:Window("Unit spawner", "Old Toilet TD [ðŸŽ„CHRISTMAS] ", "G")
local tab = w:Tab("Unit Placer must get in a game frist")

tab:Button("Place Chef TV Man",function()
  local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local unitName = "Chef TV Man" -- put unit here
local spawnDistance = -0 -- Distance behind the player to spawn the unit

local spawnPosition = humanoidRootPart.CFrame * CFrame.new(0, 0, spawnDistance)

local args = {
    [1] = unitName,
    [2] = spawnPosition
}

game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("SpawnTower"):InvokeServer(unpack(args))
end)

